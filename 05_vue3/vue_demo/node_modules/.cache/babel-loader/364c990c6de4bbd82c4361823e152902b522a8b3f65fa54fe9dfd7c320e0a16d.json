{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// reactive不能用基本数据类型，ref可以\nimport { reactive } from 'vue';\nexport default {\n  setup() {\n    const name = '马云';\n    const age = 50;\n    const taobao = '淘宝';\n    const obj = reactive({\n      taobao,\n      name,\n      age,\n      pro: {\n        a: {\n          b: ['我是深层次的数据']\n        }\n      }\n    });\n    const arr = reactive(['a', 'b', 'c']);\n    function btnEvent() {\n      obj.name = '马化腾';\n      obj.pro.a.b[0] = '深层次数据已经被改了';\n      arr.push('d');\n      console.log(obj);\n    }\n    const checkedNames = ref([]);\n    return {\n      btnEvent,\n      obj,\n      arr,\n      checkedNames\n    };\n  }\n};","map":{"version":3,"mappings":";AAmBA;AACA,SAASA,QAAO,QAAS,KAAK;AAC9B,eAAgB;EACZC,KAAK,GAAG;IACJ,MAAMC,IAAG,GAAI,IAAI;IACjB,MAAMC,GAAE,GAAI,EAAE;IACd,MAAMC,MAAK,GAAI,IAAI;IACnB,MAAMC,GAAE,GAAIL,QAAQ,CAAC;MAACI,MAAM;MACxBF,IAAI;MACJC,GAAG;MACHG,GAAG,EAAE;QACDC,CAAC,EAAE;UACCC,CAAC,EAAC,CAAC,UAAU;QACjB;MACJ;IACJ,CAAC,CAAC;IAEF,MAAMC,GAAE,GAAIT,QAAQ,CAAC,CAAC,GAAG,EAAE,GAAG,EAAE,GAAG,CAAC;IAEpC,SAASU,QAAQ,GAAG;MAChBL,GAAG,CAACH,IAAG,GAAI,KAAK;MAChBG,GAAG,CAACC,GAAG,CAACC,CAAC,CAACC,CAAC,CAAC,CAAC,IAAI,YAAY;MAC7BC,GAAG,CAACE,IAAI,CAAC,GAAG;MACZC,OAAO,CAACC,GAAG,CAACR,GAAG;IACnB;IACA,MAAMS,YAAW,GAAIC,GAAG,CAAC,EAAE;IAC3B,OAAO;MAACL,QAAQ;MAAEL,GAAG;MAAEI,GAAG;MAAEK;IAAY;EAC5C;AACJ,CAAC","names":["reactive","setup","name","age","taobao","obj","pro","a","b","arr","btnEvent","push","console","log","checkedNames","ref"],"sourceRoot":"","sources":["/Users/issuser/Desktop/Project/Web/05_vue3/vue_demo/src/components/vue3-003.vue"],"sourcesContent":["<template>\n    <h1>姓名: {{obj.name}}</h1>\n    <h1>年龄: {{obj.age}}</h1>\n    <h1>深层次数据响应:{{obj.pro.a.b[0]}}</h1>\n    <h1>产品: {{obj.taobao}}</h1>\n    <hr>\n    <button @click=\"btnEvent\">点击更新视图层某一个数据</button>\n    <p>{{ arr }}</p>\n\n    <div>Checked names: {{ checkedNames }}</div>\n        <input type=\"checkbox\" id=\"jack\" value=\"Jack\" v-model=\"checkedNames\" />\n        <label for=\"jack\">Jack</label>\n        <input type=\"checkbox\" id=\"john\" value=\"John\" v-model=\"checkedNames\" />\n        <label for=\"john\">John</label>\n        <input type=\"checkbox\" id=\"mike\" value=\"Mike\" v-model=\"checkedNames\" />\n        <label for=\"mike\">Mike</label>\n</template>\n\n<script>\n// reactive不能用基本数据类型，ref可以\nimport { reactive } from 'vue';\nexport default ({\n    setup() {\n        const name = '马云';\n        const age = 50;\n        const taobao = '淘宝';\n        const obj = reactive({taobao, \n            name, \n            age,\n            pro: {\n                a: {\n                    b:['我是深层次的数据']\n                }\n            }\n        });\n\n        const arr = reactive(['a', 'b', 'c'])\n\n        function btnEvent() {\n            obj.name = '马化腾';\n            obj.pro.a.b[0] = '深层次数据已经被改了';\n            arr.push('d')\n            console.log(obj)\n        }\n        const checkedNames = ref([])\n        return {btnEvent, obj, arr, checkedNames}\n    },\n})\n\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}