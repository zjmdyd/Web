{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup() {\n    // 从vuex数据仓库里面取数据\n    const store = useStore();\n    const res = computed(() => {\n      console.log('子组件computed钩子函数被调用');\n      console.log(store.state.name);\n      return store.state.name;\n    });\n\n    // 点击调用vuex并且改变vuex仓库里的数据\n    function btnEvent() {\n      // 异步调用:dispatch\n      store.dispatch('sub', '雷军');\n      // 同步调用\n      // store.commit('trigger')  // 不带参的\n      // store.commit('trigger', '雷军') // 带参的，各种数据类型都可以传\n    }\n\n    return {\n      res,\n      btnEvent\n    };\n  }\n};","map":{"version":3,"mappings":"AAcA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAgB;EACdC,KAAK,GAAG;IACN;IACA,MAAMC,KAAI,GAAIF,QAAQ,EAAC;IACvB,MAAMG,GAAE,GAAIJ,QAAQ,CAAC,MAAM;MACzBK,OAAO,CAACC,GAAG,CAAC,oBAAoB;MAChCD,OAAO,CAACC,GAAG,CAACH,KAAK,CAACI,KAAK,CAACC,IAAI;MAC5B,OAAOL,KAAK,CAACI,KAAK,CAACC,IAAG;IACxB,CAAC;;IAED;IACA,SAASC,QAAQ,GAAG;MAClB;MACAN,KAAK,CAACO,QAAQ,CAAC,KAAK,EAAE,IAAI;MAC1B;MACA;MACA;IACF;;IACA,OAAO;MAAEN,GAAG;MAAEK;IAAS;EACzB;AACF,CAAC","names":["computed","useStore","setup","store","res","console","log","state","name","btnEvent","dispatch"],"sourceRoot":"","sources":["/Users/issuser/Desktop/Project/Web/05_vue3/vue_demo/src/components/vue3-011.vue"],"sourcesContent":["<!-- 安装vuex -->\n<!-- npm install vuex@next --save -->\n\n<template>\n  <div class=\"back\">\n    这是子组件\n    <h2>姓名: {{ res }}</h2>\n    <br>\n    <button @click=\"btnEvent\">点击改变vuex数据仓库里面的数据</button>\n  </div>\n</template>\n\n<script>\n\nimport { computed } from 'vue'\nimport { useStore } from 'vuex'\n\nexport default ({\n  setup() {\n    // 从vuex数据仓库里面取数据\n    const store = useStore()\n    const res = computed(() => {\n      console.log('子组件computed钩子函数被调用')\n      console.log(store.state.name)\n      return store.state.name\n    })\n\n    // 点击调用vuex并且改变vuex仓库里的数据\n    function btnEvent() {\n      // 异步调用:dispatch\n      store.dispatch('sub', '雷军')\n      // 同步调用\n      // store.commit('trigger')  // 不带参的\n      // store.commit('trigger', '雷军') // 带参的，各种数据类型都可以传\n    }\n    return { res, btnEvent }\n  },\n})\n\n</script>\n\n<style scoped>\n.back {\n  background-color: blueviolet;\n  color: white;\n  padding: 20px 0;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}