{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nexport default {\n  setup() {\n    // 从vuex数据仓库里面取数据\n    const store = useStore();\n    const res = computed(() => {\n      console.log(store.state.name);\n      return store.state.name;\n    });\n\n    // 点击调用vuex并且改变vuex仓库里的数据\n    function btnEvent() {\n      // 异步调用:dispatch\n      // store.dispatch('sub')\n      // 同步调用\n      // store.commit('trigger')  // 不带参的\n      store.commit('trigger', '雷军'); // 带参的\n    }\n\n    return {\n      res,\n      btnEvent\n    };\n  }\n};","map":{"version":3,"mappings":"AAcA,SAASA,QAAO,QAAS,KAAI;AAC7B,SAASC,QAAO,QAAS,MAAK;AAE9B,eAAe;EACXC,KAAK,GAAG;IACJ;IACA,MAAMC,KAAI,GAAIF,QAAQ,EAAC;IACvB,MAAMG,GAAE,GAAIJ,QAAQ,CAAC,MAAI;MACrBK,OAAO,CAACC,GAAG,CAACH,KAAK,CAACI,KAAK,CAACC,IAAI;MAC5B,OAAOL,KAAK,CAACI,KAAK,CAACC,IAAG;IAC1B,CAAC;;IAED;IACA,SAASC,QAAQ,GAAG;MAChB;MACA;MACA;MACA;MACAN,KAAK,CAACO,MAAM,CAAC,SAAS,EAAE,IAAI,GAAE;IAClC;;IACA,OAAO;MAACN,GAAG;MAAEK;IAAQ;EACzB;AACJ,CAAC","names":["computed","useStore","setup","store","res","console","log","state","name","btnEvent","commit"],"sourceRoot":"","sources":["/Users/issuser/Desktop/vue-demo/src/components/vue3-011.vue"],"sourcesContent":["<!-- 安装vuex -->\n<!-- npm install vuex@next --save -->\n\n<template>\n    <div class=\"back\">\n        这是子组件\n        <h2>姓名: {{res}}</h2>\n        <br>\n        <button @click=\"btnEvent\">点击改变vuex数据仓库里面的数据</button>\n    </div>\n</template>\n\n<script>\n\nimport { computed } from 'vue'\nimport { useStore } from 'vuex'\n\nexport default({\n    setup() {\n        // 从vuex数据仓库里面取数据\n        const store = useStore()\n        const res = computed(()=>{\n            console.log(store.state.name)\n            return store.state.name\n        })\n\n        // 点击调用vuex并且改变vuex仓库里的数据\n        function btnEvent() {\n            // 异步调用:dispatch\n            // store.dispatch('sub')\n            // 同步调用\n            // store.commit('trigger')  // 不带参的\n            store.commit('trigger', '雷军') // 带参的\n        }\n        return {res, btnEvent}\n    },\n})\n\n</script>\n\n<style scoped>\n.back {\n    background-color: blueviolet;\n    color: white;\n    padding: 20px 0;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}